{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["dom", "dom.iterable", "es2020"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "outDir": "./dist/server",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["server/**/*.ts"],
  "exclude": ["node_modules"]
}
